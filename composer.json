{
	"name" : "localbranding-de/membershiplevels-bundle",
	"type" : "contao-bundle",
	"description" : "Hello World extension for Contao Open Source CMS",
	"license" : "LGPL-3.0-or-later",
	"authors" : [{
			"name" : "Localbranding-de",
			"homepage" : "https://github.com/localbranding-de"
		}
	],
	"require" : {
		"php" : "^5.6 || ^7.0",
		"contao/core-bundle" : "4.4.*",
		"symfony/framework-bundle" : "^3.3"
	},
	"conflict" : {
		"contao/core" : "*",
		"contao/manager-plugin" : "<2.0 || >=3.0"
	},
	"require-dev" : {
		"contao/manager-plugin" : "^2.0",
		"doctrine/doctrine-cache-bundle" : "^1.3",
		"friendsofphp/php-cs-fixer" : "^2.6",
		"leofeyer/optimize-native-functions-fixer" : "^1.1",
		"php-http/guzzle6-adapter" : "^1.1",
		"php-http/message-factory" : "^1.0.2",
		"phpunit/phpunit" : "^5.7.26",
		"symfony/phpunit-bridge" : "^3.2"
	},
	"extra" : {
		"contao-manager-plugin" : "LocalbrandingDe\\MembershiplevelsBundle\\ContaoManager\\Plugin"
	},
	"autoload" : {
		"psr-4" : {
			"LocalbrandingDe\\MembershiplevelsBundle\\" : "src/"
		},
		"classmap" : [
			"src/Resources/contao/"
		],
		"exclude-from-classmap" : [
			"src/Resources/contao/config/",
			"src/Resources/contao/dca/",
			"src/Resources/contao/docs/",
			"src/Resources/contao/languages/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"LocalbrandingDe\\MembershiplevelsBundle\\Tests\\" : "tests/"
		}
	},
	"support" : {
		"issues" : "https://github.com/localbranding-de/membershiplevels-bundle/issues",
		"source" : "https://github.com/localbranding-de/membershiplevels-bundle"
	}
}
